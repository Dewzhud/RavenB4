shared.RavenB4Started = true
local RavenConfig = loadstring(game:HttpGet('https://raw.githubusercontent.com/goinglikeatrainlol/RavenB4Roblox/main/Raven%20NoModules%20Loader'))()
shared.WaterMark = "Black Bird Config"
shared.WaterMarkColor = Color3.fromRGB(255,255,255) -- Put any RGB Value you want here!

-- สร้าง Toggle สำหรับ Infinite Jump
Module = Combat:CreateToggle({
    Name = "auto Sprint",
    StartingState = false,
    Callback = function(isEnabled)
        if isEnabled then
local args = {
    [1] = "Input",
    [2] = "HumanoidControls",
    [3] = 0,
    [4] = "Sprint"
}

game:GetService("Players").LocalPlayer.Character.Combat.RemoteEvent:FireServer(unpack(args))
        end
    end
})

Module = Combat:CreateToggle({
    Name = "lock all mobs",
    StartingState = false,
    Callback = function(isEnabled)
        if isEnabled then
local NPCsFolder = game.Workspace.NPCs 
local LoopTime = 0.9 

-- Anchor และลูปทุกรัปเฟรม
while true do
    for _, mob in pairs(NPCsFolder:GetChildren()) do
        if mob:IsA("Model") and mob:FindFirstChild("HumanoidRootPart") then
 
            mob:SetPrimaryPartCFrame(mob.HumanoidRootPart.CFrame * CFrame.new(0, 0, 0))
            mob.PrimaryPart.Anchored = true
        end
    end
    wait(LoopTime) 
end
        end
    end
})

Module = Combat:CreateToggle({
    Name = "spam",
    StartingState = false,
    Callback = function(isEnabled)

        if isEnabled then
            while true do

                local args = {
                    [1] = "Input",
                    [2] = "Charon's Tome",
                    [3] = 0,
                    [4] = "Cast",
                    [5] = 0.7753139075999828
                }
                
                game:GetService("Players").LocalPlayer.Character.Combat.RemoteEvent:FireServer(unpack(args))
          
   wait(0.5)
    end
        end
    end
})
