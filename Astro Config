
shared.WaterMark = "Astro"
shared.WaterMarkColor = Color3.fromRGB(255,255,255) -- Put any RGB Value you want here!
shared.RavenB4Started = true
local RavenConfig = loadstring(game:HttpGet('https://raw.githubusercontent.com/goinglikeatrainlol/RavenB4Roblox/main/Raven%20NoModules%20Loader'))()

-- สร้างหน้าต่าง Main
Main = RavenConfig:CreateWindow("Main", UDim2.new(0.131578952, 0, 0.0475435816, 0), 0, "Combat")

Module = Main:CreateToggle({
    Name = "Infinite Jump Mobile",
    StartingState = false,
    Callback = function(isEnabled)
        local player = game.Players.LocalPlayer
        local userInputService = game:GetService("UserInputService")

        if isEnabled then
            -- สร้างตัวแปรสำหรับตรวจสอบสถานะการกระโดด
            local isJumping = false

            -- ตั้งค่าการกระโดดเมื่อผู้เล่นแตะที่หน้าจอ
            userInputService.TouchTap:Connect(function()
                isJumping = true
                while isEnabled and isJumping do
                    player.Character:FindFirstChildOfClass("Humanoid"):Move(Vector3.new(0, 10, 0))
                    wait()
                end
            end)

            -- ตั้งค่าการหยุดกระโดดเมื่อผู้เล่นปล่อยหน้าจอ
            userInputService.TouchEnded:Connect(function()
                isJumping = false
            end)
        end
    end
})
